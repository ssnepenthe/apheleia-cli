{
    "name": "ssnepenthe/toy-wp-cli",
    "description": "An alternative syntax for writing WP-CLI commands",
    "license": "MIT",
    "type": "library",
    "require-dev": {
        "php-di/invoker": "^2.3",
        "phpunit/phpunit": "^9.5",
        "wp-cli/wp-cli-bundle": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "ToyWpCli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ToyWpCLi\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    },
    "extra": {
        "composer-normalize": {
            "indent-size": 4,
            "indent-style": "space"
        }
    }
}
