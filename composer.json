{
    "name": "ssnepenthe/apheleia-cli",
    "description": "An alternative syntax for writing WP-CLI commands",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "php-di/invoker": "^2.3",
        "php-stubs/wp-cli-stubs": "^2.6",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.22",
        "wp-cli/php-cli-tools": "^0.11.6",
        "wp-cli/wp-cli": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "ApheleiaCli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApheleiaCli\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    },
    "extra": {
        "composer-normalize": {
            "indent-size": 4,
            "indent-style": "space"
        }
    }
}
